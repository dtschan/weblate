if [ -e $OPENSHIFT_DATA_DIR/.install || -e $OPENSHIFT_DATA_DIR/.installed ]; then
	sed -e 's/Django[<>=].*/Django==1.6/' $OPENSHIFT_REPO_DIR/requirements.txt >$OPENSHIFT_DATA_DIR/requirements.txt
else
	>$OPENSHIFT_DATA_DIR/requirements.txt
fi

export OPENSHIFT_PYTHON_REQUIREMENTS_PATH=$OPENSHIFT_DATA_DIR/requirements.txt
