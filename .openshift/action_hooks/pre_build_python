if [ -e $OPENSHIFT_DATA_DIR/.install ] || [ -e $OPENSHIFT_DATA_DIR/.installed ]; then
	sed -e 's/Django[<>=].*/Django==1.6/' $OPENSHIFT_REPO_DIR/requirements.txt >$OPENSHIFT_REPO_DIR/requirements-openshift.txt
else
	>$OPENSHIFT_REPO_DIR/requirements-openshift.txt
fi

export OPENSHIFT_PYTHON_REQUIREMENTS_PATH=requirements-openshift.txt
